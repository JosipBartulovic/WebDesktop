{
    "manifest_version": 2,
  
    "name": "Getting started example",
    "description": "This extension allows the user to change the background color of the current page.",
    "version": "1.0",
  
    "browser_action": {
      "default_popup": "popup/popup.html"
    },
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "new_tab.html"
    }
  }