{
    "name": "NewTab",
    "permissions": ["tabs"],
    "browser_action":{
    "default_popup": "./popup/dist/popup.html"
    },
    "description": "My super cool new tab",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
    "128": "./app/dist/assets/icon_128.png"
    },
    "chrome_url_overrides" : { "newtab": "./app/dist/index.html"},
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}